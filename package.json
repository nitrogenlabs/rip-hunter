{
  "name": "rip-hunter",
  "version": "1.3.1",
  "description": "JS utilities for AJAX and GraphQL",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "ajax",
    "fetch",
    "graphql",
    "nitrogenlabs",
    "rip-hunter",
    "typescript"
  ],
  "author": "Giraldo Rosales <giraldo@nitrogenlabs.com> (http://nitrogenlabs.com/)",
  "repository": {
    "type": "git",
    "url": "http://github.com/nitrogenlabs/rip-hunter"
  },
  "homepage": "https://github.com/nitrogenlabs/rip-hunter",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/rip-hunter/issues"
  },
  "engines": {
    "node": ">6.0.0",
    "npm": ">3.3.6"
  },
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prepublishOnly": "NODE_ENV=production npm test && npm run compile",
    "pretest": "npm run lint",
    "reset": "rm -rf node_modules yarn.lock",
    "test": "jest",
    "update": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/fetch-mock": "^6.0.1",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "fetch-mock": "^6.1.0",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "tslint": "^5.6.0",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/src/**/*test.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "verbose": false
  }
}